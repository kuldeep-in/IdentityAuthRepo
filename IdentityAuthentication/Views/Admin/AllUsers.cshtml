@model IEnumerable<IdentityAuthentication.Data.IdentityAuthUser>
@{
    ViewData["Title"] = "Index";
}

<table class="table table-striped" style="margin-top:10px;">
    <thead class="thead-light">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Email</td>
                <td>
                    @if (item.Email != "user1@email.com")
                    {
                        @Html.ActionLink("Delete", "DeleteRole", new { id = item.Id }, new { @class = "btn btn-danger", @onclick = "return confirm('Are you sure you want to delete?')" })
                    }
                </td>
            </tr>
        }
    </tbody>
</table>